{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "/Applications/Godot_mono.app/Contents/MacOS/Godot",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Attach",
            "type": "mono",
            "request": "attach",
            "address": "localhost",
            "port": 23685,
            "internalConsoleOptions": "neverOpen",
        }
    ],
    "compounds": [
        {
            "name": "Run and attach",
            "configurations": [
                "Attach",
                "Run",
            ]
        }
    ]
}